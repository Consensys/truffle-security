    "@truffle/error": "0.0.7",
    "@truffle/expect": "0.0.12",
    "@truffle/external-compile": "^1.0.19",
    "@truffle/resolver": "^5.0.18",
    "armlet": "^2.7.0",
    "configstore": "^4.0.0",
